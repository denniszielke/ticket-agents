# GitHub Configuration
GITHUB_TOKEN=your_github_token_here
GITHUB_REPO=owner/repository

# OpenAI Configuration (option 1 - direct OpenAI)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini

# Azure OpenAI Configuration (option 2 - Azure OpenAI)
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com
AZURE_OPENAI_API_KEY=your_azure_openai_key_here
AZURE_OPENAI_EMBEDDING_MODEL=text-embedding-3-small
AZURE_OPENAI_VERSION=2024-02-15

# Azure AI Project Configuration (option 3 - Azure AI Project with agents)
AZURE_AI_PROJECT_ENDPOINT=https://your-project.api.azureml.ms

# Observability (optional)
APPLICATIONINSIGHTS_CONNECTION_STRING=your_app_insights_connection_string

# Application Configuration
INDEX_FILE=ticket_index.json
COMPLETION_MODEL_NAME=gpt-4o-mini
